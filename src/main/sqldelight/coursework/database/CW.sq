CREATE TABLE LECTURER (
 id INTEGER PRIMARY KEY AUTOINCREMENT,
NAME TEXT NOT NULL,
AGE INTEGER NOT NULL,
STATUS TEXT ,
GENDER TEXT
);

allLecturers:
SELECT *
FROM LECTURER;

CREATE TABLE FACULTY (
 id INTEGER PRIMARY KEY AUTOINCREMENT,
 NAME TEXT NOT NULL
);

allFaculties:
SELECT *
FROM FACULTY;

insertLecturer:
INSERT INTO LECTURER ( NAME, AGE, STATUS, GENDER) VALUES ( ?, ?, ?, ?);

insertFaculty:
INSERT INTO FACULTY (NAME) VALUES (?);